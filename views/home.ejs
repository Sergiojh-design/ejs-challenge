<%- include('partials/header'); -%>
<h1>Home</h1>
<p><%= startingContent %></p>
<% postsArray.forEach(function(post){ %>
  <h1><%- post.postTitle  %></h1>
 <% if(post.postBody.length > 100){ %>
  <p><%- post.postBody.slice(0,100)%>...</p>
 <% } else { %>
  <p><%- post.postBody  %></p>
 <% } %>
<% }); %>
<%- include('partials/footer'); -%>