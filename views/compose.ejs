<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="POST">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="postTitle" placeholder="">
    <label>Post</label>
    <textarea class="form-control" type="text" name="postBody" placeholder="" rows="4"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include('partials/footer'); -%>